<h1>New Camper Form</h1>
<% if @camper.errors.any? %>
    <% @camper.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
    <%end %>
<% end %>


<%= form_for @camper do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :age %>
    <%= f.number_field :age %><br>

    <%= f.submit %>
<% end %>